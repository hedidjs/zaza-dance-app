#include "Pods/Target Support Files/Pods-Zaza Dance/Pods-Zaza Dance.release.xcconfig"
#include "Generated.xcconfig"

// iOS Deployment Target
IPHONEOS_DEPLOYMENT_TARGET = 13.0

// App Configuration
PRODUCT_BUNDLE_IDENTIFIER = com.zazadance.zazaDance
DEVELOPMENT_TEAM = 

// Explicit Flutter Configuration
FLUTTER_FRAMEWORK_DIR = $(FLUTTER_ROOT)/bin/cache/artifacts/engine/ios/Flutter.xcframework
FLUTTER_BUILD_MODE = release
DART_OBFUSCATION = false
TREE_SHAKE_ICONS = false

// Performance and Stability Settings
ENABLE_BITCODE = NO
SWIFT_OPTIMIZATION_LEVEL = -O
GCC_OPTIMIZATION_LEVEL = s

// Memory Management
CLANG_ENABLE_OBJC_ARC = YES
CLANG_ANALYZER_NONNULL = YES

// Release Optimizations and dSYM Generation
DEBUG_INFORMATION_FORMAT = dwarf-with-dsym
DWARF_DSYM_FOLDER_PATH = $(CONFIGURATION_BUILD_DIR)
DWARF_DSYM_FILE_NAME = $(EXECUTABLE_NAME).dSYM
DEPLOYMENT_POSTPROCESSING = YES
STRIP_INSTALLED_PRODUCT = YES
ONLY_ACTIVE_ARCH = NO
VALIDATE_PRODUCT = YES
// Force dSYM generation for proper crash symbolication
GCC_GENERATE_DEBUGGING_SYMBOLS = YES
STRIP_STYLE = debugging
COPY_PHASE_STRIP = NO
GENERATE_PROFILING_CODE = NO

// Comprehensive Warnings Suppression
CLANG_WARN_DEPRECATED_OBJC_IMPLEMENTATIONS = NO
GCC_WARN_ABOUT_DEPRECATED_FUNCTIONS = NO
CLANG_WARN_OBJC_IMPLICIT_ATOMIC_PROPERTIES = NO
GCC_WARN_ABOUT_MISSING_PROTOTYPES = NO
CLANG_WARN_DOCUMENTATION_COMMENTS = NO
CLANG_WARN_UNGUARDED_AVAILABILITY = NO
CLANG_WARN_OBJC_IMPLICIT_RETAIN_SELF = NO
CLANG_WARN_IMPLICIT_SIGN_CONVERSION = NO
CLANG_WARN_ASSIGN_ENUM = NO
CLANG_WARN_UNREACHABLE_CODE = NO
CLANG_WARN_COMMA = NO
CLANG_WARN_STRICT_PROTOTYPES = NO
CLANG_WARN_NULLABLE_TO_NONNULL_CONVERSION = NO
CLANG_WARN_OBJC_LITERAL_CONVERSION = NO
CLANG_WARN_RANGE_LOOP_ANALYSIS = NO
CLANG_WARN_SUSPICIOUS_MOVE = NO
GCC_WARN_ABOUT_RETURN_TYPE = NO
GCC_WARN_ABOUT_MISSING_NEWLINE = NO
GCC_WARN_SIGN_COMPARE = NO
GCC_WARN_UNUSED_FUNCTION = NO
GCC_WARN_UNUSED_LABEL = NO
GCC_WARN_UNUSED_PARAMETER = NO
GCC_WARN_UNUSED_VALUE = NO
GCC_WARN_UNUSED_VARIABLE = NO
GCC_WARN_SHADOW = NO
GCC_WARN_64_TO_32_BIT_CONVERSION = NO
GCC_WARN_ABOUT_INVALID_OFFSETOF_MACRO = NO
GCC_WARN_ABOUT_POINTER_SIGNEDNESS = NO
GCC_WARN_ALLOW_INCOMPLETE_PROTOCOL = NO
GCC_WARN_INITIALIZER_NOT_FULLY_BRACKETED = NO
GCC_WARN_MISSING_PARENTHESES = NO
GCC_WARN_TYPECHECK_CALLS_TO_PRINTF = NO
GCC_WARN_UNINITIALIZED_AUTOS = NO

// Swift Warnings Suppression  
SWIFT_SUPPRESS_WARNINGS = YES
SWIFT_TREAT_WARNINGS_AS_ERRORS = NO

// Additional iOS-specific Warnings Suppression
WARNING_CFLAGS = -Wno-deprecated-declarations -Wno-unused-variable -Wno-unused-function -Wno-unused-parameter -Wno-sign-compare -Wno-conversion -Wno-shorten-64-to-32 -Wno-conditional-uninitialized -Wno-sometimes-uninitialized -Wno-unreachable-code -Wno-deprecated-implementations

// Disable specific iOS warnings
CLANG_WARN_OBJC_ROOT_CLASS = NO
CLANG_WARN_DIRECT_OBJC_ISA_USAGE = NO

// Essential Xcode Recommended Settings (minimal for compatibility)
ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = NO
ASSETCATALOG_COMPILER_GENERATE_SWIFT_ASSET_SYMBOL_EXTENSIONS = YES
ENABLE_PARALLELIZATION_IN_COMMAND_LINE_BUILDS_USING_TARGET = YES
CLANG_WARN_QUOTED_INCLUDE_IN_FRAMEWORK_HEADER = NO
